{
  "name": "linktree",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://github.com/investingwithrain/linktree",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "echo \"APP_VERSION=$npm_package_version\" > .env",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "git add . && git commit -m 'auto push' && git push origin main && echo links.investingwithrain.com > ./dist/CNAME && gh-pages -d dist",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.4",
    "@hookstate/core": "^4.0.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/styled-engine-sc": "^6.1.6",
    "@mui/styles": "^6.3.1",
    "chart.js": "^4.4.7",
    "firebase": "^11.1.0",
    "gh-pages": "^5.0.0",
    "google-fonts": "^1.0.0",
    "jsdom": "^23.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.21.0",
    "remark-gfm": "^4.0.0",
    "styled-components": "^6.1.13",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.3.9",
    "vite-plugin-md": "^0.22.5"
  }
}
